<shapes>
<shape name="state" aspect="variable" strokewidth="7" >
	<background>
		<strokecolor color="#333333"/>
		<fillcolor color="#ffffff"/>
		<ellipse x="0" y="0" w="100" h="100" />	
		<fillstroke/>
	</background>
	<foreground>
		<fillstroke/>
	</foreground>
</shape>
<shape name="initialState" aspect="variable" strokewidth="14" >
	<background>
		<strokecolor color="#333333"/>
		<fillcolor color="#ffffff"/>
		<ellipse x="0" y="0" w="100" h="100" />	
		<fillstroke/> 
	</background>
	<foreground>
		<fillstroke/>
	</foreground>
</shape>
<shape name="transition" aspect="variable" strokewidth="7" >
	<background>
		<strokecolor color="#333333"/>
		<fillcolor color="#ffffff"/>
		<path>
			<move x="0" y="0"/> 
			<line x="90" y="0"/> 
			<line x="100" y="50"/> 
			<line x="90" y="100"/> 
			<line x="0" y="100"/> 
			<line x="10" y="50"/> 
			<line x="0" y="0"/> 
			<close/>
		</path>		
	</background>
	<foreground>
		<fillstroke/>
	</foreground>
</shape>
</shapes>