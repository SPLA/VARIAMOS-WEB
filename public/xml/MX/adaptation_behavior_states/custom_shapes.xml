<shapes>
<shape name="state" aspect="variable" strokewidth="7" >
	<background>
		<strokecolor color="#333333"/>
		<fillcolor color="#ffffff"/>
		<ellipse x="0" y="0" w="100" h="100" />	
		<fillstroke/>
	</background>
	<foreground>
		<fillstroke/>
	</foreground>
</shape>
<shape name="initialState" aspect="variable" strokewidth="14" >
	<background>
		<strokecolor color="#333333"/>
		<fillcolor color="#ffffff"/>
		<ellipse x="0" y="0" w="100" h="100" />	
		<fillstroke/> 
	</background>
	<foreground>
		<fillstroke/>
	</foreground>
</shape>
<shape name="activity" aspect="variable" strokewidth="7">
	<background>
		<strokecolor color="#333333"/>
		<fillcolor color="#ffffff"/>
		<path>
			<move x="0" y="0"/> 
			<line x="100" y="0"/>  
			<line x="100" y="100"/>  
			<line x="0" y="100"/> 
			<line x="0" y="0"/>   
			<close/>
			<move x="0" y="10"/>
			<line x="100" y="10"/> 
			<close/>
			<move x="0" y="90"/>
			<line x="100" y="90"/> 
			<close/>
		</path>		
	</background>
	<foreground>
		<fillstroke/>
	</foreground>
</shape> 
<shape name="lifeLine" aspect="variable" strokewidth="7">
	<background>
		<strokecolor color="#333333"/>
		<fillcolor color="#ffffff"/>
		<path>
			<move x="0" y="0"/> 
			<line x="100" y="0"/>  
			<line x="100" y="100"/>  
			<line x="0" y="100"/> 
			<line x="0" y="0"/>   
			<close/> 
		</path>		
	</background>
	<foreground>
		<fillstroke/>
	</foreground>
</shape> 
<shape name="controlAction" aspect="variable"  strokewidth="7" > 
	<background>
		<strokecolor color="#333333"/>
		<fillcolor color="#ffffff"/>
		<path>
			<move x="75" y="0"/> 
			<line x="100" y="50"/>  
			<line x="75" y="100"/> 
			<line x="25" y="100"/> 
			<line x="0" y="50"/> 
			<line x="25" y="0"/>  
			<line x="75" y="0"/> 
			<close/>
		</path>		
	</background>
	<foreground>
		<fillstroke/>
	</foreground>
</shape> 
<shape name="writeAction" aspect="variable"  strokewidth="7" > 
	<background>
		<strokecolor color="#333333"/>
		<fillcolor color="#ffffff"/>
		<path>
			<move x="20" y="0"/> 
			<line x="100" y="0"/>  
			<line x="80" y="100"/>  
			<line x="0" y="100"/> 
			<line x="20" y="0"/>   
			<close/> 
		</path>		
	</background>
	<foreground>
		<fillstroke/>
	</foreground>
</shape> 
<shape name="readAction" aspect="variable"  strokewidth="7" >
	<background>
		<strokecolor color="#333333"/>
		<fillcolor color="#ffffff"/>
		<path>
			<move x="0" y="50"/> 
			<line x="100" y="0"/>  
			<line x="100" y="100"/>  
			<line x="0" y="100"/> 
			<line x="0" y="50"/>   
			<close/> 
		</path>		
	</background>
	<foreground>
		<fillstroke/>
	</foreground>
</shape> 
</shapes>