<shapes>
<!-- begin component model -->
<shape name="package" aspect="variable">
	<background>
		<rect x="0" y="20" w="100" h="80"/>
		<fillstroke/>
		<rect x="0" y="0" w="45" h="20"/>
		<fillstroke/>
   </background>
	<foreground>
		<fillstroke/>
	</foreground>
</shape>

<shape name="component" aspect="variable">
	<background>
		<path>
			<move x="15" y="0"/>
			<line x="100" y="0"/>
			<line x="100" y="100"/>
			<line x="15" y="100"/>
			<line x="15" y="0"/>
			<close/>
			<move x="0" y="25"/>
			<line x="30" y="25"/>
			<line x="30" y="40"/>
			<line x="0" y="40"/>
			<line x="0" y="25"/>
			<close/>
			<move x="0" y="60"/>
			<line x="30" y="60"/>
			<line x="30" y="75"/>
			<line x="0" y="75"/>
			<line x="0" y="60"/>
			<close/>
		</path>		
	</background>
	<foreground>
		<fillstroke/>
	</foreground>
</shape>

<shape name="file" aspect="variable">
	<background>
		<path>
			<move x="0" y="0"/>
			<line x="70" y="0"/>
			<quad x1="70" y1="0" x2="100" y2="30"/>
			<line x="100" y="100"/>
			<line x="0" y="100"/>
			<line x="0" y="0"/>
			<close/>
			<move x="70" y="0"/>
			<line x="70" y="30"/>
			<line x="100" y="30"/>
			<close/>
		</path>		
	</background>
	<foreground>
		<fillstroke/>
	</foreground>
</shape>

<shape name="fragment" aspect="variable">
