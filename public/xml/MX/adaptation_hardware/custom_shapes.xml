<shapes>

<shape name="board" aspect="variable" strokewidth="1" >
	<background>
		<strokecolor color="#333333"/>
		<fillcolor color="#ffffff"/>
		<path>
			<move x="00" y="00"/>
			<line x="100" y="00"/>  
			<line x="100" y="100"/> 
			<line x="00" y="100"/> 
			<close/>  
		</path>	
	</background>
	<foreground>
		<fillstroke/>
	</foreground>
</shape>

